<%- if @projeto.errors.any? %>
  $('#dialog-form').html('<%= escape_javascript(render('form')) %>');
  var qtd_errors = <%= @projeto.errors.count %>;
  var tam = (650 + (qtd_errors * 12) );
  $('#dialog-form').css("height",tam + "px");
  $('.ui-dialog').css("top","10px");
  $('#cliente-select-id').focus();
  $("#project-form-tabs").tabs();
  $("#sortable").sortable();
  inicializaTinyMCE();
  setMaskFields();
  bindProjetoUIEvents();
  bindGerenciamentoCamposFrequenciaEDuracaoTreinamento();
  $("#cliente-select-carrega-solucao").change(function(){
    carregaSolucoesCliente();
  });
<%- else %>
  $('#dialog-form').dialog('close');
  $('#dialog-form').remove();
  $(window.location.replace("/usuario/projetos/<%= @projeto.id %>"));

<%- end %>
