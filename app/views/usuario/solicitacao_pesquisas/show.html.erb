<% title 'dsad' %>
  <table border="0"  cellspacing="1" cellpadding="0">
    <thead>	
      <tr>
        <th>Cliente</th>
        <th>Título</th>
        <th>Sub Módulo</th>
        <th>Status </th> 
        <th width = "70">Prioridade</th>
        <th width ="120">Tipo</th>
        <th width ="30">Dias</th>
        <th width ="10">Show</th>
      </tr>
    </thead>
    <tbody>
      <% for solicitacao in @solicitacoes %>
        <tr>
          <td><%=h solicitacao.atendimento.cliente.nome %></td>
          <td><%=h solicitacao.titulo %></td>	
          <td><%=h solicitacao.solucao_sub_modulo.descricao %></td>
          <td><%=h solicitacao.status.descricao %> </td>
          <td><%=h solicitacao.prioridade.descricao %></td>
          <td><%=h solicitacao.tipo_pendencia.descricao %></td>
          <td><%=h 1 %></td>
          <td><%= link_to "Show", solicitacao,:class => 'show' %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  
