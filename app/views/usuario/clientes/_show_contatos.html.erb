<div id="aba-cliente-contatos">
  <table border="0" cellpadding="0" cellspacing="1" style="width:99%;margin-left:12px;" >
    <caption>Contatos do Cliente </caption>
    <thead>
      <tr>
        <th width ="200" >Nome</th>
        <th>Email</th>
        <th width ="110">Telefone</th>
        <th width ="110">Celular</th>
        <th width ="10">Edit</th>
        <th width ="10">Del</th>
      </tr>
    </thead>
    <tbody>
      <% for cliente_contato in @cliente_contatos %>
        <% @cliente = cliente_contato.cliente %>	
        <tr>
          <td>
            <%=link_to cliente_contato.nome,
                usuario_cliente_contato_path(:id => cliente_contato.id, :local => :cliente_show),
                class:"inline-link" %>
          </td>
          <td><%=h cliente_contato.email %></td>
          <td><%=h cliente_contato.telefone %></td>
          <td><%=h cliente_contato.celular %></td>
          <td><%= link_to image_tag(image_link[:edit]), 
                  edit_usuario_cliente_contato_path(cliente_contato,:local => :cliente_show) %></td>
          
          <td><%= link_to image_tag(image_link[:del]),  
                      [:usuario,cliente_contato], 
                      :confirm => 'Confirma ExclusÃ£o ?',
                      :method => :delete %></td>
        </tr>
      <% end %>
     </tbody> 
  </table>

  <p>
    <li class ="links">
      <%= link_to 'Novo Contato',
        new_usuario_cliente_contato_path(:cliente => @cliente,:local => :cliente_show),
        class:'button-add-new add-on-index-top-with-header' %>
    </li>
  </p>
  <br><br><br>
</div>