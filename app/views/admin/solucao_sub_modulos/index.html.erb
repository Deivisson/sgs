<% title "Sub Módulos" %>

<table border ="0" cellspacing ="1" cellpadding ="0" >
  <thead>
    <tr>
      <th width ="200">Solucao </th>
      <th width ="200">Módulo</th>
      <th>Descrição Sub Módulo</th>
      <th width ="10">Det</th>
      <th width ="10">Edit</th>
      <th width ="10">Del</th>
    </tr>
  </thead>
  <tbody>  
    <% for solucao_sub_modulo in @solucao_sub_modulos %>
      <tr>
        <td><%=h solucao_sub_modulo.solucao %></td>
        <td><%=h solucao_sub_modulo.modulo %></td>
        <td><%=h solucao_sub_modulo.sub_modulo %></td>
        <td><%= link_to image_tag(image_link[:show]), [:admin, solucao_sub_modulo] %></td>

        <td><%= link_to image_tag(image_link[:edit]), 
                  edit_admin_solucao_sub_modulo_path(solucao_sub_modulo) %></td>

        <td><%= link_to image_tag(image_link[:del]), 
                      [:admin, solucao_sub_modulo], 
                      :confirm => 'Confirma Exclusão?', :method => :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= will_paginate @solucao_sub_modulos, :id => 'paginate' %>

<p>
  <li class ="links"> 
    <%= link_to 'Sub Módulo',new_admin_solucao_sub_modulo_path,:class => 'new' %>
  </li>
</p>
