<table border ="0" cellspacing ="1" cellpadding = "0" style="width:99%" >
  <caption>Módulos do Solucao </caption>
  <thead>
    <tr>
      <th>Descrição</th>
      <th width ="10">Det</th>
      <th width ="10">Edit</th>
      <th width ="10">Del</th>
    </tr>
  </thead>
  <tbody>
    <% for solucao_modulo in @solucao_modulos %>
      <tr>
        <td><%=h solucao_modulo.descricao %></td>
        <td><%= link_to image_tag(image_link[:show]), 
                        admin_solucao_modulo_path(:id => solucao_modulo.id ,:local => :solucao_show)   %></td>
                        
        <td><%= link_to image_tag(image_link[:edit]), 
                        edit_admin_solucao_modulo_path(solucao_modulo,:local => :solucao_show) %></td>
                        
        <td><%= link_to image_tag(image_link[:del]), admin_solucao_modulo_path, 
                        :confirm => 'Confirma Exclusão?', :method => :delete %></td>
      </tr>
    <% end %>
  <tbody>
</table>

