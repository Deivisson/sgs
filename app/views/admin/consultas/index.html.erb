<% title "Consultas do Sistema " %>
<table border ="0" cellpadding ="0" cellspacing ="1" >
  <thead>
    <tr>
      <th>Descrição</th>
      <th width="10">Det</th>
      <th width="10">Edit</th>
      <th width="10">Camp</th>
    </tr>
  </thead>
  <tbody>
    <% for consulta in @consultas %>
      <tr>
        <td><%=h consulta.descricao %></td>
        <td><%= link_to image_tag(image_link[:show]), [:admin,consulta] %></td>
        <td><%= link_to image_tag(image_link[:edit]), edit_admin_consulta_path(consulta) %></td>
        <td><%= link_to image_tag(image_link[:new]) ,admin_consulta_campos_path(:consulta => consulta.id)  %></td>
      </tr>
    <% end %>
  </tbody>
</table>

